package com {		import flash.display.MovieClip;	import fl.video.FLVPlayback;	import flash.events.MouseEvent;			public class DeplaceTeteParam extends MovieClip {				private var ecran:FLVPlayback;				public function DeplaceTeteParam() {						ecran = VideoUtils.chargeVideo("_flv/BLIS.flv",384,216,8,8);			addChild(ecran);						btInstant1.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant);			btInstant2.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant);			btInstant3.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant);						btInstant1.temps=2;			btInstant2.temps=5;			btInstant3.temps=7;		}				private function allerInstant(_e:MouseEvent) {			ecran.seek(_e.currentTarget.temps);		}	}	}