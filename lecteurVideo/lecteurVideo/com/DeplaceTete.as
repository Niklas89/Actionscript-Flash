package com {		import flash.display.MovieClip;	import fl.video.FLVPlayback;	import flash.events.MouseEvent;		public class DeplaceTete extends MovieClip {				private var ecran	:FLVPlayback;				public function DeplaceTete() {						ecran = VideoUtils.chargeVideo("_flv/BLIS.flv",384,216,8,8);			addChild(ecran);						btInstant1.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant1);			btInstant2.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant2);			btInstant3.addEventListener(MouseEvent.MOUSE_DOWN,allerInstant3);		}				/* La méthode seek() permet de déplacer la tête de lecture		à un temps précis exprimé en secondes*/				private function allerInstant1(evt:MouseEvent) {			ecran.seek(2);		}				private function allerInstant2(evt:MouseEvent) {			ecran.seek(5);		}				private function allerInstant3(evt:MouseEvent) {			ecran.seek(7);		}	}	}