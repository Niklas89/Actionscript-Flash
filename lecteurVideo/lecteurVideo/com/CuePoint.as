package com {		import flash.display.MovieClip;	import fl.video.MetadataEvent;	import fl.video.FLVPlayback;		public class CuePoint extends MovieClip {				private var ecran	:FLVPlayback;				public function CuePoint() {			ecran = VideoUtils.chargeVideo("_flv/BLIS.flv",384,216,8,8);			addChild(ecran);						ecran.addASCuePoint(2,"Vue sur la ville");			ecran.addASCuePoint(10,"Mise en action du BLIS");			ecran.addASCuePoint(15,"Schema de fonctionnement");			ecran.addASCuePoint(20,"");						ecran.addEventListener(MetadataEvent.CUE_POINT,repereAtteint);					}		private function repereAtteint(_e:MetadataEvent) {			messageVideo.text = _e.info.name;		}	}	}