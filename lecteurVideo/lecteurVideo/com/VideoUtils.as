 package  com {		import fl.video.FLVPlayback;		public class VideoUtils {		private static var maVideo:FLVPlayback;				public static function chargeVideo(_url:String,_l:uint,_h:uint,											_x:uint,_y:uint,											skinUrl:String = null):FLVPlayback		{			maVideo			= new FLVPlayback();			skinUrl ? maVideo.skin = skinUrl : null;			maVideo.source 	= _url;			maVideo.x 		= _x;			maVideo.y 		= _y;			maVideo.setSize(_l,_h);			return maVideo;		}	}	}