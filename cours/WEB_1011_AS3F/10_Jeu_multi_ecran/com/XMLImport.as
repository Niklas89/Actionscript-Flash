package com {	import flash.display.Sprite;	import flash.events.Event;	import flash.net.URLLoader;	import flash.net.URLRequest;		public class XMLImport extends Sprite {				public 	var xmlData:XML;		private var vObjetParent:Object;						public function XMLImport(pXmlFile:String, pObjetParent:Object):void {						vObjetParent = pObjetParent;						var xmlURL:URLRequest 		= new URLRequest(pXmlFile);			var xmlLoader:URLLoader 	= new URLLoader(xmlURL);						xmlLoader.addEventListener(Event.COMPLETE, xmlLoaded);		}				private function xmlLoaded(p:Event):void {			xmlData 				= new XML(p.target.data);			vObjetParent.xmlCallBack();		}	}}