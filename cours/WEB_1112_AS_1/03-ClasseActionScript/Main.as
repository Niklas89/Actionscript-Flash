package {	import flash.display.MovieClip;	import flash.events.Event;		import com.ClasseOccurence;	public class Main extends MovieClip	{		private var p:Poisson;				public function Main()		{			init();		}		private function init():void		{			p = new Poisson();			p.x = 250;			p.y = 100;			addChild(p);						poisson.addEventListener(Event.ENTER_FRAME, onPoisson);		}				private function onPoisson(e:Event = null):void		{			e.target.x +=  e.target.mouseX / 25;			e.target.y +=  e.target.mouseY / 25;		}	}}