package com {	import flash.display.MovieClip;	import flash.display.DisplayObjectContainer;	import flash.events.Event;	public class ClasseOccurence extends MovieClip {		public function ClasseOccurence(sp:DisplayObjectContainer) {			// "sp" défini le scenario principal			// stage etant de type "DisplayObjectContainer"			// on choisi "sp" comme etant de type "DisplayObjectContainer"						// instanciation de la classe poisson depuis la classe d'occurrence			var poisson:Poisson = new Poisson();			poisson.y = 200;						sp.addChild(poisson);		}	}} 