package com.niklas {		import flash.display.MovieClip;	import flash.events.Event;			public class Poisson extends MovieClip {						public function Poisson() {			this.addEventListener(Event.ADDED_TO_STAGE, onAddToStage);		}		private function onAddToStage(e:Event):void		{			this.addEventListener(Event.ENTER_FRAME, loop);		}		private function loop(e:Event):void		{			this.x += this.mouseX /16;		}	}	}